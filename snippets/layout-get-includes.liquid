    {%- if settings.tooltips_enable -%}
    Loader.require({type: 'style', name: 'plugin_tippy'});
    {%- endif -%}
    Loader.require({type: 'style', name: 'layout'});
    Loader.require({type: 'style', name: 'theme'});
